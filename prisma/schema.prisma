
generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @map("_id")
  name String
  email String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
